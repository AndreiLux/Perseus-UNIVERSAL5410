Import( 'env' )

libs=env.StaticLibrary( '$STATIC_LIB_PATH/uku', ['mali_uku_linux.c'] )

env.LibTarget('uku', libs)

if env.has_key('libs_install'):
	env.Install (env['libs_install'], libs)
	env.Alias ('libs', env['libs_install'])
else:
	env.Alias ('libs', libs)
